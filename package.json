{
  "name": "js-http-status",
  "version": "0.1.1",
  "description": "[![License][license-badge]][license-url]",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=dev webpack --progress --colors --watch",
    "build": "NODE_ENV=production webpack",
    "lint": "eslint src tests",
    "test": "jest --coverage --expand",
    "test:watch": "jest --watch",
    "prepublish": "yarn lint && yarn test && yarn clean && yarn build",
    "coveralls": "cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "copy-docs": "cp -rf docs/js-http-status/**/* ./docs",
    "generate-docs": "jsdoc --configure .jsdocrc --verbose -r && yarn copy-docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codevor/js-http-status.git"
  },
  "keywords": [
    "javascript",
    "es6",
    "library",
    "http-status",
    "js-http-status"
  ],
  "author": "Helder Burato Berto <helder.burato@gmail.com> (https://helder.dev/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codevor/js-http-status/issues"
  },
  "homepage": "https://github.com/codevor/js-http-status#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.7",
    "cz-conventional-changelog": "3.0.2",
    "docdash": "^1.1.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.3",
    "rimraf": "^3.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
